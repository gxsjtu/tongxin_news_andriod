<?xml version="1.0" encoding="utf-8"?>
<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#c9c9ce"
    android:orientation="vertical">
    <include layout="@layout/header"></include>
<ScrollView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="vertical">
    <LinearLayout android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

    <com.daimajia.slider.library.SliderLayout
        android:id="@+id/sq_imgSlider"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        />
    <LinearLayout
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"   
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center"
        android:padding="5dp"
        >
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_name"
            android:layout_marginLeft="15dp"
            />
    <TextView
        android:id="@+id/sq_detailName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
    </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_qty"
            android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/sq_detailQty"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_price"
            android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/sq_detailPrice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_contact"
            android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/sq_detailContact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_mobile"
            android:layout_marginLeft="15dp"/>

    <TextView
        android:id="@+id/sq_detailMobile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:textColor="#000"
        android:layout_marginLeft="10dp"
        android:gravity="left"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_location"
            android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/sq_detailLocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:gravity="center">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_deliver"
            android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/sq_detailDeliver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textColor="#000"
        android:enabled="false"
        />
        </LinearLayout>
    <LinearLayout
        android:padding="5dp"
        android:background="@drawable/linearlayoutconer"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true">
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/sq_desc"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:id="@+id/sq_detailDesc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_marginLeft="10dp"
            android:textColor="#000"
            android:enabled="false"
            />
        </LinearLayout>
        </LinearLayout>
</ScrollView>
    <include layout="@layout/loading"></include>
</LinearLayout>